{
  "name": "trail-blazer",
  "version": "0.1.0",
  "description": "Drone flight planner.",
  "repository": "https://github.com/prodec/trail-blazer",
  "readme": "https://github.com/prodec/trail-blazer/README.md",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^5.1.11",
    "babel-eslint": "^3.1.20",
    "babel-loader": "^5.0.0",
    "classnames": "^2.1.2",
    "css-loader": "^0.11.0",
    "es6-promise": "^1.0.0",
    "eslint": "^0.24.0",
    "eslint-plugin-babel": "^1.0.0",
    "eslint-plugin-react": "^2.6.3",
    "file-loader": "^0.8.1",
    "flux": "^2.0.1",
    "google-maps-api": "^1.0.2",
    "html-loader": "^0.2.3",
    "html-webpack-plugin": "^1.3.0",
    "jsx-loader": "^0.13.1",
    "keymirror": "^0.1.1",
    "node-sass": "^3.0.0-beta.4",
    "object-assign": "^1.0.0",
    "proj4": "^2.3.6",
    "proj4js-defs": "0.0.1",
    "purecss": "^0.6.0",
    "react": "^0.13.2",
    "react-hot-loader": "^1.2.5",
    "sass-loader": "^1.0.1",
    "style-loader": "^0.11.0",
    "url-loader": "^0.5.5",
    "webpack": "^1.7.2",
    "webpack-dev-server": "^1.7.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "babel",
      "react"
    ],
    "rules": {
      "no-undef": 1,
      "no-underscore-dangle": 0,
      "no-unused-vars": 1,
      "no-use-before-define": 0,
      "quotes": [
        1,
        "single"
      ],
      "strict": 0,
      "babel/object-shorthand": 1,
      "babel/generator-star": 1,
      "babel/generator-star-spacing": 1,
      "react/display-name": [
        1,
        {
          "acceptTranspilerName": true
        }
      ],
      "react/jsx-boolean-value": 1,
      "react/jsx-no-undef": 1,
      "react/jsx-quotes": [
        1,
        "double"
      ],
      "react/jsx-sort-prop-types": 1,
      "react/jsx-sort-props": 0,
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
      "react/no-did-mount-set-state": 1,
      "react/no-did-update-set-state": 1,
      "react/no-multi-comp": 0,
      "react/no-unknown-property": 1,
      "react/prop-types": 0,
      "react/react-in-jsx-scope": 1,
      "react/require-extension": 1,
      "react/self-closing-comp": 1,
      "react/sort-comp": 0,
      "react/wrap-multilines": 1
    },
    "ecmaFeatures": {
      "arrowFunctions": true,
      "binaryLiterals": true,
      "blockBindings": true,
      "classes": true,
      "defaultParams": true,
      "destructuring": true,
      "forOf": true,
      "generators": true,
      "modules": true,
      "objectLiteralComputedProperties": true,
      "objectLiteralDuplicateProperties": true,
      "objectLiteralShorthandMethods": true,
      "objectLiteralShorthandProperties": true,
      "octalLiterals": true,
      "regexUFlag": true,
      "regexYFlag": true,
      "restParams": true,
      "spread": true,
      "superInFunctions": true,
      "templateStrings": true,
      "unicodeCodePointEscapes": true,
      "globalReturn": true,
      "jsx": true
    },
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "globals": {
      "L": false
    }
  }
}
